      }
    });
    // alert(tab+" - "+field);
    // alert(JSON.stringify(id_array));
    
  }

  function add_custom_field(id)
  {
    field = id;
    data = "&field="+field
    $.ajax({
      data: data,
      type: 'POST',
      url: '/custom/show_custom_field',
