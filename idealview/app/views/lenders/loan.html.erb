                            </div>
                          </div>

            </div>

            <% unless @new_custom_fields.blank? %>
              <div class="content active" id="panel5">
                <div class="content-white-box">
                  <div class="cwb-header">
                      <h3>Custom Fields</h3>
                  </div>
                  <% t = 1 %>
                  <div class="cwb-content">
                      <div class="row">
                        
                        <% @new_custom_fields.each do |custom_field|%>  
                          <% unless @checkif.include? "#{custom_field.field_name}" %>
                            <% 
                              border=""
                              if t%2 != 0 
                                border = "border-right"
                              end
                              t += 1
                                %>
                            <div class="small-6 columns <%= border %>">
                              <div class="trans-info-item">
                                <div class="row">
                                    <div class="small-12 columns">
                                      <strong><%= custom_field.name %>:</strong>
                                      <br><%= @loan.info[custom_field.field_name] %>
                                    </div>
                                  </div>
                              </div>
                            </div>
                          <% end %>
                        <% end %>
                      
                      </div>
                  </div>
                </div><!-- content-white-box -->
              </div>
            <% end %>

          </div>
      <% end %>

    </div>
  </div>
</div><!-- content-area -->


