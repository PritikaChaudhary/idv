<% if @borrowers.empty? %>
	<input type="button" value="Add Borrower" class="button" id="add_new_borrower">
<% else %>
	<input type="button" value="Add More Borrower" class="button" id="add_new_borrower">
<% end %>

<div id="all_new_borrowers">
<% serial_num=1 %>
	<% if !@borrowers.empty? %>
		<% @borrowers.each do |borrower| %>
		
		<script type="text/javascript">
			$('document').ready(function(){
				<% if borrower.borrower_type=="Company or Trust" %> 
		        	$('#borrow_<%= borrower.id %> .business_info').css({"display":"inline-block"});
		        	$('#borrow_<%= borrower.id %> .business_financials').css({"display":"inline-block"});
		        	$('#borrow_<%= borrower.id %> .personal_info').css({"display":"none"});
		      	 	$('#borrow_<%= borrower.id %> .borrower_bio').css({"display":"none"});
		      	<% else %>
		        	$('#borrow_<%= borrower.id %> .business_info').css({"display":"none"});
		        	$('#borrow_<%= borrower.id %> .business_financials').css({"display":"none"});
		      	<% end %>

		      	<% if borrower.borrower_type=="Individual" && borrower.borrower_guarantor=="No"  %>
		      	<% else %>
		      		$('#borrow_<%= borrower.id %> .guarantor_info').css({"display":"none"});
		      	<% end %>

			});
		
		</script>
			<div class="borrower_space" style="visibility:hidden; height:33px;"></div>
