											<option value="Company or Trust" <% if borrower.borrower_type == "Company or Trust" %> selected="selected" <% end %>>Corporate Borrower & Personal Guarantor(s)</option>
										</select>
					                    <div class="tii-controls">
					                        <%= render partial: 'show_hide_borrower', locals:{borwr: borrower, field_name:"borrower_type"}%>
					                    </div>
					                </div>
					        </div>

					        <div class="personal_info" <% if borrower.borrower_type == "Company or Trust" %> style="display:none;" <% end %>>
