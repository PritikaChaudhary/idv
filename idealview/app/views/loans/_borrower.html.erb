									<img src="/assets/eye-icon-slash.png">
								</a>
							</div>

							<div id="delete_<%= borrower['_id'] %>" >
								<a style=" float:right;" onclick="delete_borrower('<%= borrower['_id'] %>');" title = "Delete">
									<i class="fa fa-trash" aria-hidden="true"></i>
								</a>
							</div>


				        </div>
