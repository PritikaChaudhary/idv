    else if(id == "document")
    {
      tab_val = "Document"
    }
    else if(id == "custom")
    {
      tab_val = "Custom Fields"
    }
    data = "tab="+tab_val
    $.ajax({
      data: data,
      type: 'POST',
      url: '/custom/add_tab',
      success:function(response)
      {
        $('#'+id).show();
        $('#add_'+id).hide();
        $('#remove_'+id).show();
      }
    });
  }

  function add_field(id)
  {
    id_array = id.split("_");
    tab = id_array[0];
    if(tab == "transaction")
    {
      tab = "transaction_information"
    }
    field = id.replace(tab+"_", "");

    data = "tab="+tab+"&field="+field
    $.ajax({
      data: data,
      type: 'POST',
      url: '/custom/add_field',
      success:function(response)
      {
        $('#'+id).show();
        $('#add_'+id).hide();
        $('#remove_'+id).show();
      }
    });
  }

  function remove_field(id)
  {
    id_array = id.split("_");
    tab = id_array[0];
    if(tab == "transaction")
    {
      tab = "transaction_information"
    }
    field = id.replace(tab+"_", "");
    data = "tab="+tab+"&field="+field
    $.ajax({
      data: data,
      type: 'POST',
      url: '/custom/remove_field',
      success:function(response)
      {
        $('#'+id).hide();
        $('#remove_'+id).hide();
