				        <div class="cwb-controls show_controls">

				        	
				            <div id="hideborrower_<%= borrower['_id'] %>" <% if borrower.hide == 1 %> style="display:none; float:right; margin-left:10px;" <% else %> style="float:right; margin-left:10px;" <% end %>>
								<a style="color:black; float:right;" onclick="hide_borrower('<%= borrower['_id'] %>');" title = "Hide">
									<img src="/assets/eye-icon.png">
								</a>
							</div>	
							
							<div id="showborrower_<%= borrower['_id'] %>"  <% if borrower.hide != 1 %> style="display:none; float:right; margin-left:10px;" <% else %> style="float:right;  margin-left:10px;" <% end %>>
