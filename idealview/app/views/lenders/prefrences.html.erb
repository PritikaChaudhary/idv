<script type="text/javascript">
	$('document').ready(function() {
		 
		$('.lend_cat').change(function(){
	        var slct = $(this).val();
	        if(slct=="Business Financing")
	        {
	            $('#lending_type').hide();
	            $('#financing_type').show();
	            $('#crowd_funding').hide();
	            $('#mortgage_types').hide();
	        }
	        if(slct=="Equity and Crowd Funding")
	        {
	            $('#lending_type').hide();
	            $('#financing_type').hide();
	            $('#crowd_funding').show();
	            $('#mortgage_types').hide();
	        }
	        if(slct=="Residential or Commercial Mortage")
	        {
	            $('#lending_type').hide();
	            $('#financing_type').hide();
	            $('#crowd_funding').hide();
	            $('#mortgage_types').show();
	        }
	        if(slct=="Private Real Estate Loan")
	        {
	            $('#lending_type').show();
	            $('#financing_type').hide();
	            $('#crowd_funding').hide();
	            $('#mortgage_types').hide();
	        }
    	});
	});

function check_email()
{
    
