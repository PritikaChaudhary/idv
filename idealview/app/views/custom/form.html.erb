<div id="loading_img" style="display:none"></div>
<script type="text/javascript">
  $('document').ready(function(){
      $('#add_new_fields').click(function(){
        $('#myModalFields').foundation('reveal', 'open');
      });

      $('#change_custom_name').click(function(){
        $('#myModalHead').foundation('reveal', 'open');
      });
    
      $('#save_field').click(function(){
       var field_name = $('#field_name').val();
       $("#loading_img").show();
       if(field_name != "")
       {
          data = "field_name="+field_name
          $('.error_field').hide();
          $('.error_already').hide();
          $.ajax({
                    data: data,
                    type: 'POST',
                    url: '/custom/check_if_exist',
                    success:function(response){
                      if(response == "yes")
                      {
                        $.ajax({
                            data: data,
                            type: 'POST',
                            url: '/custom/add_new_field',
                            success:function(response){
                              $('#all_new_custom_fields').html(response);
                              $.ajax({
                                  data: data,
                                  type: 'POST',
                                  url: '/custom/right_custom_tab',
                                  success:function(response){
                                     $('#table_custominfo').html(response)
                                     $('#myModalFields').foundation('reveal', 'close');
                                     $("#loading_img").hide();
                                  }
                              });
                              
                            }
                        });
                      }
                      else
                      {
                        $('.error_already').show();
                      }
                    }
                });

         
       }
       else
       {
          $('.error_field').show();
       }
        
    });

    $('#save_heading_field').click(function(){
       var heading_tab = $('#custom_tab_heading').val();
       $("#heading_tab").show();
       if(heading_tab != "")
       {
          data = "tab_name="+heading_tab;
          $('.heading_error').hide();
          $.ajax({
                    data: data,
                    type: 'POST',
                    url: '/custom/edit_custom_tab',
                    success:function(response){
                      if(response == "done")
                      {
                        $('#custom_heading').html(heading_tab);
                        $('#table_custom_head').html(heading_tab);
                        $('#myModalHead').foundation('reveal', 'close');
                        $("#loading_img").hide();
                      }
                      else
                      {
                        $('.heading_error').show();
                      }
                    }
                });

         
       }
       else
       {
          $('.heading_error').show();
       }
        
    });

    $('.custom_check').click(function(){
        if($(this).is(':checked'))
        {
            check_val = $(this).val();
            var data = "field="+check_val+"&required=1";  
        }
        else
        {
            check_val = $(this).val();   
            var data = "field="+check_val+"&required=0";  
        }

       $.ajax({
            data: data,
            type: 'POST',
            url: '/custom/required_custom_field',
            success:function(response)
            {
             
            }
      });
    });
  });
  

</script>

